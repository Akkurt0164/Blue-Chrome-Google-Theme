{
    "name": "Blue Chrome Theme",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A simple theme extension for Chrome.",
    "action":{
        "default_icon": "logo.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage"
    ]
}